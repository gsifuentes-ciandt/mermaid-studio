# AI Provider Configuration
VITE_AI_PROVIDER=flow
VITE_AI_FALLBACK_PROVIDER=openai

# Flow API Proxy Configuration
VITE_FLOW_PROXY_URL=http://localhost:3001/api/chat/completions

# OpenAI Configuration (Fallback)
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_OPENAI_API_URL=https://api.openai.com/v1/chat/completions

# AI Model Settings
VITE_AI_MODEL=gpt-4.1
VITE_AI_TEMPERATURE=0.7
VITE_AI_MAX_TOKENS=4000
VITE_AI_TIMEOUT=60000

# Feature Flags
VITE_AI_ENABLED=true
VITE_AI_STREAMING=false
VITE_AI_CACHE_ENABLED=true

# Supabase Configuration (for collaboration features)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
